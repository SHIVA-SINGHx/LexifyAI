generator client {
  provider = "prisma-client-js"

}

datasource db {
  provider = "postgresql"
}


model ai_output{
  id String @id @default(uuid())
  userId String
  title String?
  description String
  templateUsed String
  createdAt DateTime @default(now())

}

model purchase {
  id String @id @default(uuid())
  userId String
  credit String
  createdAt DateTime @default(now())
}

model user{
  id String @id @unique @default(uuid())
  userId String @unique
  total_credit Float @default(10000)
}