
generator client {
  provider = "prisma-client"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model ai_output{
  id String @id @default(uuid())
  userId String
  title String?
  description String
  templateUsed String
  createdAt DateTime @default(now())

}